    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Launch Ensnare DAW",
            "cargo": {
                "args": [
                  "build",
                  "--bin=ensnare-daw",
                  "--package=ensnare",
                  "--features=",
                ],
                "filter": {
                  "name": "ensnare-daw",
                  "kind": "bin"
                }
              },
              "args": [],
              "cwd": "${workspaceFolder}"
                },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'ensnare'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=ensnare"
                ],
                "filter": {
                    "name": "ensnare",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug example 'hello_world'",
            "cargo": {
                "args": [
                    "build",
                    "--example=hello_world",
                    "--package=ensnare"
                ],
                "filter": {
                    "name": "hello_world",
                    "kind": "example"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in example 'hello_world'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--example=hello_world",
                    "--package=ensnare"
                ],
                "filter": {
                    "name": "hello_world",
                    "kind": "example"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "MiniDAW example",
            "cargo": {
                "args": [
                    "build",
                    "--example=minidaw",
                    "--package=ensnare",
                ],
                "filter": {
                    "name": "minidaw",
                    "kind": "example"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "MiniCLI example",
            "cargo": {
                "args": [
                    "build",
                    "--example=minicli",
                    "--package=ensnare",
                ],
                "filter": {
                    "name": "minicli",
                    "kind": "example"
                }
            },
            "args": [
                "-w",
                "minicli-work.json"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Explorer example",
            "cargo": {
                "args": [
                    "build",
                    "--example=explorer",
                    "--package=ensnare",
                ],
                "filter": {
                    "name": "explorer",
                    "kind": "example"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
    ]
}
