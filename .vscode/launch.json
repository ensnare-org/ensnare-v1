    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Launch Ensnare DAW",
            "cargo": {
                "args": [
                    "build",
                    "--bin=ensnare-daw",
                    "--package=ensnare",
                    "--features=",
                ],
                "filter": {
                    "name": "ensnare-daw",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'ensnare'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=ensnare"
                ],
                "filter": {
                    "name": "ensnare",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug example 'hello-world'",
            "cargo": {
                "args": [
                    "build",
                    "--example=hello-world",
                    "--package=ensnare"
                ],
                "filter": {
                    "name": "hello-world",
                    "kind": "example"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug example 'Pocket Calculator'",
            "cargo": {
                "args": [
                    "build",
                    "--example=pocket-calculator",
                    "--package=ensnare"
                ],
                "filter": {
                    "name": "pocket-calculator",
                    "kind": "example"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "MiniDAW example",
            "cargo": {
                "args": [
                    "build",
                    "--example=minidaw",
                    "--package=ensnare",
                ],
                "filter": {
                    "name": "minidaw",
                    "kind": "example"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "render example",
            "cargo": {
                "args": [
                    "build",
                    "--example=render",
                    "--package=ensnare",
                ],
                "filter": {
                    "name": "render",
                    "kind": "example"
                }
            },
            "args": [
                "-w",
                "simple.json"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Widget Explorer example",
            "cargo": {
                "args": [
                    "build",
                    "--example=widget-explorer",
                    "--package=ensnare",
                ],
                "filter": {
                    "name": "widget-explorer",
                    "kind": "example"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Entity GUI Explorer example",
            "cargo": {
                "args": [
                    "build",
                    "--example=entity-gui-explorer",
                    "--package=ensnare",
                ],
                "filter": {
                    "name": "entity-gui-explorer",
                    "kind": "example"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
    ]
